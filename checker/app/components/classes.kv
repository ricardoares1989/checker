#:import Hex kivy.utils.get_color_from_hex
<MainScreenWidget>
    canvas.before:
        Color:
            rgba: Hex("#FFFFFF")
        Rectangle:
            pos: self.pos
            size: self.size

<Checks@FloatLayout>
    

<Register@FloatLayout>
    Button:
        id: guardado
        size_hint: .28, .08
        pos_hint: {"x": .14,"y": .33 }
        font_name: "arial"
        font_size: 18
        foreground_color: Hex("#FFFFFF")
        text: "Registrar"
        background_color: Hex("#FF9800")
    Button:
        id: guardar_excel
        size_hint: .28, .08
        pos_hint: {"x": .57,"y": .33 }
        font_name: "arial"
        font_size: 18
        foreground_color: Hex("#FFFFFF")
        text: "Generar Excel"
        background_color: Hex("#056839")
